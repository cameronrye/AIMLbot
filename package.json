{
  "name": "aimlbot",
  "version": "0.0.1",
  "description": "AIML powered chatbot",
  "bin": "cli.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "nyc ava",
    "lint": "standard",
    "dev": "chokidar '**/*.js' -c 'standard && nyc ava'",
    "report": "nyc report --reporter=html"
  },
  "aiml": {
    "files": [
      "aiml/name.aiml",
      "aiml/joke.aiml"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cameronrye/AIMLbot.git"
  },
  "author": "Cameron Rye <npm@cameronrye.com> (https://cameronrye.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cameronrye/AIMLbot/issues"
  },
  "homepage": "https://github.com/cameronrye/AIMLbot#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "chokidar-cli": "^1.2.0",
    "coveralls": "^2.11.15",
    "get-stream": "^3.0.0",
    "nyc": "^10.1.2",
    "precommit-hook": "^3.0.0",
    "standard": "^8.6.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "arrify": "^1.0.1",
    "meow": "^3.7.0",
    "pkg-conf": "^2.0.0"
  }
}
